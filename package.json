{
    "name": "focus-devtools",
    "version": "0.4.0-beta1",
    "description": "Focus devtools",
    "main": "index.js",
    "scripts": {
        "build": "better-npm-run babelify"
    },
    "betterScripts": {
        "babelify": {
            "command": "babel src/ --out-dir .",
            "env": {
                "BABEL_ENV": "production",
                "LEGACY_EXPORTS": "true"
            }
        }
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/kleegroup/focus-devtools.git"
    },
    "keywords": [
        "react",
        "redux",
        "data",
        "focus"
    ],
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/kleegroup/focus-devtools/issues"
    },
    "homepage": "https://github.com/kleegroup/focus-devtools#readme",
    "babel": {
        "presets": [
            "focus"
        ]
    },
    "peerDependencies": {
        "react": "15.4.2",
        "react-dom": "15.4.2"
    },
    "devDependencies": {
        "babel-preset-focus": "1.0.0-beta1",
        "eslint-config-focus": "0.6.0",
        "react": "15.4.2",
        "react-dom": "15.4.2"
    },
    "dependencies": {
        "redux": "3.7.2",
        "react-redux": "5.0.6",
        "react-json-tree": "0.10.9",
        "redux-thunk": "2.2.0",
        "react-dock": "0.2.4",
        "redux-logger": "3.0.6",
        "parse-key": "0.2.1"
    }
}